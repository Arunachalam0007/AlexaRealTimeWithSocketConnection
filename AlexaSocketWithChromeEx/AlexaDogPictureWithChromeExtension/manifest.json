{
    "name": "xCP Notification",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Notification will be received from your case management system..!",
    "browser_action": {
        "default_icon": "xcp.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "notifications",
        "tabs"
    ],
    "web_accessible_resources": [
        "xcp.png"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}